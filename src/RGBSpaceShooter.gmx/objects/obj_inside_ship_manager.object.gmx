// destroyable ships
_count_to_create_ship ++
if (_count_to_create_ship == 500) {
    _create_ship = true
    _count_to_create_ship = 0
    _count_to_create_barriers = 0
}

// landed ships
if (_create_ship &amp;&amp; !_stop_ship_indicator) {
    var iy = -65
    for (i=0; i&lt;10; i++) {
        instance_create(333,iy, obj_red_ship_1_inside)
        iy += -64 
    }
    _create_ship = false
}

if (instance_exists(obj_red_ship_1_inside)&amp;&amp; !_stop_ship_indicator) {
    _create_barrier = false
} else {
    _create_barrier = true
}    

if (_create_barrier &amp;&amp; !_stop_ship_indicator) {
    bx = irandom_range(330,600)
    by = -64
    _count_to_create_barriers ++
    if (_count_to_create_barriers == 50) { 
       instance_create(bx, by, obj_barrier_1)
       _count_to_create_barriers = 0
    }
}

// exit ship
if (_stop_ship_indicator) {
    _count_to_show_exit ++
    if (_count_to_show_exit == 80) {
        instance_create(380,-900, obj_barrier_1)
        instance_create(700,-900, obj_barrier_1)
        instance_create(room_width/2-20,-800, obj_exit_ship)
    }
}

// Create cannons
cx = choose(110, 905)
_count_to_create_cannon ++
if (_count_to_create_cannon == 100) { 
   var base = instance_create(cx, by, obj_cannon_base_2)
   _count_to_create_cannon = 0
}

// Move ship indicador
if (!_stop_ship_indicator) {
    _count_to_move_indicator ++
    if (_count_to_move_indicator == 100) {
        _meters_moved += 1
        obj_inside_ship_indicator.y -= 8    
        _count_to_move_indicator = 0
        if (obj_inside_ship_indicator.y &lt; 240) {
            _stop_ship_indicator = true
        }
    }
}


// First counter to create enemies
if (_count_to_enemy == 0) {
    _count_to_enemy ++
    if (_count_to_enemy == 100) {
        _create_ship_13 = true
    }   
}    

if (_create_ship_13) {
    r = irandom_range(1,3)
    if (r==1) {
        _ship_13_red_instance = instance_create(0,0, obj_red_ship_13)
    }
    if (r==2) {
       _ship_13_green_instance = instance_create(0,0, obj_green_ship_13)
    }
    if (r==3) {
       _ship_13_blue_instance = instance_create(0,0, obj_blue_ship_13)
    }
    _create_ship_13 = false
}

// Verify if all ships 13 were destroyed then flag to create new
if (
    !instance_exists(_ship_13_red_instance) &amp;&amp;
    !instance_exists(_ship_13_green_instance) &amp;&amp;
    !instance_exists(_ship_13_blue_instance) &amp;&amp; 
    !_create_ship_13) {
    _create_ship_13 = true  
}

