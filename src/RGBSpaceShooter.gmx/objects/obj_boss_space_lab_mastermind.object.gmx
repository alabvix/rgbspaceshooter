// Starts with attack 1, then count to launch attack 2
if (!_launch_attack_2 &amp;&amp; _launch_attack_1) {

    if (_count_to_laser == 0) {
        obj_enemy_antenna._fire = true
    } else {
        obj_enemy_antenna._fire = false
    }
    
    _count_to_laser ++
    if (_count_to_laser == _laser_limit) {
        _count_to_laser = 0
    }

    _count_to_attack_2 ++
    if (_count_to_attack_2 == 200) {
        sprite_index = spt_boss_space_lab_master_mind_bolt
        _launch_attack_2 = true
        _launch_attack_1 = false
        _count_to_attack_2 = 0
        //BossAttackBolt()
    }
}

if (_launch_attack_2 &amp;&amp; !_launch_attack_1) {

    // create bolt storm
    if (_count_to_bolt == 0) {
        var bolt = choose(obj_bolt_1, obj_bolt_2)
        instance_create(BOLT_MAP[_index,1], BOLT_MAP[_index,2], bolt)
    }
    _count_to_bolt ++
    if (_count_to_bolt == _bolt_limit) {
        _count_to_bolt = 0
        _index ++
        if (_index &gt; 9){
            _index = 1
        }
    }

    _count_to_attack_1 ++
    if (_count_to_attack_1 == 200) {
        _launch_attack_2 = false
        _launch_attack_1 = true
        _count_to_attack_1 = 0
        sprite_index = spt_boss_space_lab_master_mind
    }
}


